+void FileSelect_DrawFileSeedInfo(GameState* thisx, Vtx* vertices, s16 fileIndex) {
+    FileSelectState* this = (FileSelectState*)thisx;
+
+    OPEN_DISPS(this->state.gfxCtx, __FILE_NAME__, __LINE__);
+
+    gDPPipeSync(POLY_OPA_DISP++);
+
+    gDPSetCombineLERP(POLY_OPA_DISP++, PRIMITIVE, ENVIRONMENT, TEXEL0, ENVIRONMENT, TEXEL0, 0, PRIMITIVE, 0, PRIMITIVE,
+                  ENVIRONMENT, TEXEL0, ENVIRONMENT, TEXEL0, 0, PRIMITIVE, 0);
+    gDPSetEnvColor(POLY_OPA_DISP++, 0, 0, 0, 0);
+
+    gSPVertex(POLY_OPA_DISP++, vertices, 12, 0);
+
+    int seedButtonAlpha = 0;
+    int seedTextboxAlpha = 0;
+    if (this->configMode >= CM_MAIN_TO_RANDOMIZER) {
+        seedButtonAlpha = 255;
+        if (this->seed[fileIndex]) {
+            seedTextboxAlpha = 200;
+        }
+    } else {
+        if (this->seed[fileIndex]) {
+            seedButtonAlpha = seedTextboxAlpha = this->fileInfoAlpha[fileIndex];
+
+            if (seedButtonAlpha == 200) {
+                seedButtonAlpha = 255;
+            }
+        }
+    }
+
+    gDPSetPrimColor(POLY_OPA_DISP++, 0, 0, sWindowContentColors[0][0], sWindowContentColors[0][1],
+                    sWindowContentColors[0][2], seedButtonAlpha);
+
+    gDPLoadTextureBlock(POLY_OPA_DISP++, sRandomizerSettingButtonTextures[0], G_IM_FMT_IA, G_IM_SIZ_16b,
+            64, 16, 0, G_TX_NOMIRROR | G_TX_WRAP, G_TX_NOMIRROR | G_TX_WRAP, G_TX_NOMASK, G_TX_NOMASK,
+            G_TX_NOLOD, G_TX_NOLOD);
+    gSP1Quadrangle(POLY_OPA_DISP++, 0, 2, 3, 1, 0);
+
+    gDPSetPrimColor(POLY_OPA_DISP++, 0, 0, sWindowContentColors[0][0], sWindowContentColors[0][1],
+        sWindowContentColors[0][2], seedTextboxAlpha);
+
+    gDPLoadTextureBlock(POLY_OPA_DISP++, gFileSelNameBoxTex, G_IM_FMT_IA, G_IM_SIZ_16b, 108, 16, 0,
+            G_TX_NOMIRROR | G_TX_WRAP, G_TX_NOMIRROR | G_TX_WRAP, G_TX_NOMASK, G_TX_NOMASK, G_TX_NOLOD,
+            G_TX_NOLOD);
+
+    gSP1Quadrangle(POLY_OPA_DISP++, 4, 6, 7, 5, 0);
+
+    gDPSetPrimColor(POLY_OPA_DISP++, 0, 0, sWindowContentColors[0][0], sWindowContentColors[0][1],
+        sWindowContentColors[0][2], seedTextboxAlpha ? seedButtonAlpha : 0);
+
+    gDPLoadTextureBlock(POLY_OPA_DISP++, gFileSelConnectorTex, G_IM_FMT_IA, G_IM_SIZ_8b, 24, 16, 0,
+            G_TX_NOMIRROR | G_TX_WRAP, G_TX_NOMIRROR | G_TX_WRAP, G_TX_NOMASK, G_TX_NOMASK, G_TX_NOLOD,
+            G_TX_NOLOD);
+
+    gSP1Quadrangle(POLY_OPA_DISP++, 8, 10, 11, 9, 0);
+
+    gDPSetPrimColor(POLY_OPA_DISP++, 0, 0, 255, 255, 255, seedTextboxAlpha ? seedButtonAlpha : 0);
+
+    // Can only load 32 vertices at once, split into two iterations
+    Font* font = &this->font;
+    int vOffset = 0;
+    int startVtx = 12;
+    for (int half = 0; half < 2; half++, startVtx += 20, vOffset = 0) {
+        gSPVertex(POLY_OPA_DISP++, &vertices[startVtx], 20, 0);
+
+        for (int offset = 0; offset < 5; offset++, vOffset += 4) {
+            FileSelect_DrawCharacter(this->state.gfxCtx,
+                     font->fontBuf + (this->seedChars[fileIndex][offset + (5 * half)]) * FONT_CHAR_TEX_SIZE, vOffset);
+        }
+    }
+
+    CLOSE_DISPS(this->state.gfxCtx, __FILE_NAME__, __LINE__);
+}
