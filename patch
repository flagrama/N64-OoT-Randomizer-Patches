+#include "assets/overlays/ovl_file_choose/gRandoMenuSettingsVtx.inc.c"
+};
+
+Vtx gRandoFileInfoVtx[] = {
+#include "assets/overlays/ovl_file_choose/gRandoFileInfoVtx.inc.c"
